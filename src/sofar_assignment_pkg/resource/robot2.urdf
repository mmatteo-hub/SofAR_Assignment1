<?xml version="1.0" ?>
<robot name="MyRbt2">
    <webots>
        <device reference="ds00" type="DistanceSensor">
            <ros>
                <topicName>left_sensor</topicName>
                <alwaysOn>true</alwaysOn>
            </ros>
        </device>
        
        <device reference="ds10" type="DistanceSensor">
            <ros>
                <topicName>right_sensor</topicName>
                <alwaysOn>true</alwaysOn>
            </ros>
        </device>
        
        <plugin type="sofar_assignment_pkg.robot_driver2.RobotDriver2"/>
    </webots>
</robot>